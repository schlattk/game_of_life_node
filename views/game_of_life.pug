doctype html

head
  meta(charset='utf-8')
  link(rel='stylesheet', href='../stylesheets/style.css')
  title Game_of_Life
h1 GAME OF LIFE
form(action='/',method='POST')
      input#start(type='submit', method='POST', action='/', value='run', name='move')
      input#stop(type='submit', method='POST', action='/', value='stop', name='move')
      input#reset(type='submit', method='POST', action='/', value='startreset', name='move')
container
  div#grid
      each val in display
           != val
           br
p(id="data")
 script(src="/socket.io/socket.io.js")
 script.
   var socket = io();

   socket.on('data', function(data) {
      let grid = document.getElementById("grid");
      grid.innerHTML = "";
      data.forEach((array) =>{grid.insertAdjacentHTML('afterbegin',array + "<br>")});
   });
